@{
    ViewBag.Main = "Master Data";
    ViewBag.Category = "Master";
    ViewBag.Parent = "MasterJasa";
    ViewBag.Title = "Jenis Kendaraan";
}


@section styles {
    <link rel="stylesheet" href="~/css/datagrid/datatables/datatables.bundle.css" />
    <link rel="stylesheet" href="~/lib/sweetalert/sweetalert2.min.css" />
    <link rel="stylesheet" href="~/css/fab.css" />
}

<!--Modal Window-->
<div id='myModal' class='modal fade in' data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div id='myModalContent'></div>
        </div>
    </div>
</div>

<div class="row">    
    <div class="col-xl-12">        
        <div id="panel-1" class="panel">
            <div class="panel-hdr bg-primary-700 bg-success-gradient" role="heading">
                <h2>Master Data Jenis Kendaraan</h2>                
            </div>
            <div class="panel-container show">
                <div id="listContent" class="panel-content">
                    <table id="tblData" class="table table-striped table-bordered" data-page-length="5">
                        <thead>
                            <tr>
                                <th style="width: 15%;">ID Jenis</th>
                                <th>Global ID</th>
                                <th>Nama Jenis Kendaraan</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<button id="floating-button" class="showMe" data-href="/master/kendaraan/jenis/create">+</button>

@section scripts {
    <script src="~/lib/sweetalert/sweetalert2.all.min.js"></script>
    <script src="~/js/datagrid/datatables/datatables.bundle.js"></script>
    <script src="~/pages/master/jasa/jeniskendaraan.js"></script>
    <script src="~/js/modalForm.js"></script>
    <script>
        $('#js-page-content').smartPanel();        
    </script>
}