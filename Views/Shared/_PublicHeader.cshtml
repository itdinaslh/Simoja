<!-- Header
		============================================= -->
<header id="header" class="full-header transparent-header" data-sticky-class="not-dark">
	<div id="header-wrap">
		<div class="container">
			<div class="header-row">

				<!-- Logo
				============================================= -->
				<div id="logo">
					<a href="/" class="standard-logo" data-dark-logo="~/web/images/logo/logo-dinas.png"><img src="~/web/images/logo/logo-dinas.png" class="py-2" alt="Logo"></a>
					<a href="/" class="retina-logo" data-dark-logo="~/web/images/logo/logo-dinas.png"><img src="~/web/images/logo/logo-dinas.png" class="py-2" alt="Logo"></a>
				</div><!-- #logo end -->

				<div class="header-misc">

					<!-- Top Search
					============================================= -->
					<div id="top-search" class="header-misc-icon">
						<a href="#" id="top-search-trigger"><i class="icon-line-search"></i><i class="icon-line-cross"></i></a>
					</div><!-- #top-search end -->



				</div>

				<div id="primary-menu-trigger">
					<svg class="svg-trigger" viewBox="0 0 100 100"><path d="m 30,33 h 40 c 3.722839,0 7.5,3.126468 7.5,8.578427 0,5.451959 -2.727029,8.421573 -7.5,8.421573 h -20"></path><path d="m 30,50 h 40"></path><path d="m 70,67 h -40 c 0,0 -7.5,-0.802118 -7.5,-8.365747 0,-7.563629 7.5,-8.634253 7.5,-8.634253 h 20"></path></svg>
				</div>

				<!-- Primary Navigation
				============================================= -->
				<nav class="primary-menu">

					<ul class="menu-container">
						<li class="menu-item">
							<a class="menu-link" href="/"><div>Beranda</div></a>
						</li>
						<li class="menu-item">
							<a class="menu-link" asp-controller="Web" asp-action="Usaha"><div>Usaha/Kegiatan</div></a>
						</li>
						<li class="menu-item">
							<a class="menu-link" href="#"><div>Penyedia Jasa</div></a>
							<ul class="sub-menu-container">
								<li class="menu-item">
									<a class="menu-link" asp-controller="Web" asp-action="JasaAngkut"><div>Pengangkutan Sampah</div></a>
								</li>
								<li class="menu-item">
									<a class="menu-link" asp-controller="Web" asp-action="JasaOlah"><div>Pengolahan Sampah</div></a>
								</li>
							</ul>
						</li>
						<li class="menu-item">
							<a class="menu-link" asp-controller="Web" asp-action="Kontak"><div>Kontak</div></a>
						</li>
						<li class="menu-item">
							<a class="menu-link" asp-controller="Account" asp-action="Register"><div>Daftar</div></a>
						</li>
						<li class="menu-item">
							@if (User.Identity!.IsAuthenticated)
							{
								<a class="menu-link" asp-controller="Home" asp-action="Index"><div>Dashboard</div></a>
							} else
							{
								<a class="menu-link" asp-controller="Authorization" asp-action="LogIn"><div>Masuk</div></a>
							}							
						</li>
					</ul>

				</nav><!-- #primary-menu end -->

				<form class="top-search-form" action="search.html" method="get">
					<input type="text" name="q" class="form-control" value="" placeholder="Telusuri..." autocomplete="off">
				</form>

			</div>
		</div>
	</div>
	<div class="header-wrap-clone"></div>
</header><!-- #header end -->